{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceGroupName": {
      "value": "${AZURE_ENV_NAME}-rg"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "gptModel": {
      "value": "gpt-4"
    },
    "gptVersion": {
      "value": "0613"
    },
    "openaiName": {
      "value": "${AZURE_OPENAI_NAME}"
    },
    "msiName": {
      "value": "${AZURE_MSI_NAME}"
    },
    "appServicePlanName": {
      "value": "${AZURE_APP_SERVICE_PLAN_NAME}"
    },
    "appServiceName": {
      "value": "${AZURE_APP_SERVICE_NAME}"
    },
    "botServiceName": {
      "value": "${AZURE_BOT_SERVICE_NAME}"
    },
    "cosmosName": {
      "value": "${AZURE_COSMOS_NAME}"
    },
    "sqlServerName": {
      "value": "${AZURE_SQL_SERVER_NAME}"
    },
    "sqlDBName": {
      "value": "${AZURE_SQL_DB_NAME}"
    },
    "searchName": {
      "value": "${AZURE_SEARCH_NAME}"
    },
    "documentIntelligenceName": {
      "value": "${AZURE_DOCUMENT_INTELLIGENCE_NAME}"
    },
    "tags": {
      "value": {
        "Owner": "AI Team",
        "Project": "GPTBot",
        "Environment": "Dev",
        "Toolkit": "Bicep"
      }
    },
    "deployDocIntel": {
      "value": true
    },
    "deploySearch": {
      "value": true
    },
    "deploySQL": {
      "value": true
    },
    "deployDalle3": {
      "value": false
    },
    "deployBing": {
      "value": true
    }
  }
}